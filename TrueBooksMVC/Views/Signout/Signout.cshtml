@{
    Layout = "~/Views/Shared/_LoginMaster.cshtml";
}


@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)

    <div class="container">
        <div class="row">
            <div class="col-md-4 col-md-offset-4">
                <br /><br />
                 <div class="form-group">
                            <div class="alert alert-success alert-dismissable" role="alert" id="divMsgHeader" style="display:block;">
                                <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">×</span></button>
                                <h4><i class="icon fa fa-check"></i>@ViewBag.SignOut</h4>
                                <div id="divFinalMsg"></div>
                            </div>
                      
                  </div>
                <br />
                @Html.ActionLink("CLICK HERE TO LOGIN", "Login", "Login")
            </div>
        </div>
    </div>
}
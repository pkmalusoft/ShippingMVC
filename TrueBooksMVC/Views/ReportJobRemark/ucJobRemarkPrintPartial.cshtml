@model IEnumerable<DAL.Report_JobRemark_Result>

@{
    Layout = null;
    
}

<style>
    .back {
        background-color: red;
        color: white;
    }

     tfoot {
    display: table-header-group;
    }
</style>

@*<script src="~/Content/jQuery-2.1.3.min.js"></script>*@

 
<style>
    .back {
        background-color: red;
        color: white;
    }

</style>

<table border="0">
     <tr>
         <td colspan="8" style="text-align:center"><h3>Job Remark Report</h3></td>
     </tr>
</table>


    <table border="1" style="width:980px">
      
            <tr>
                  <th>Job_No</th>
                <th>Job Date</th>
                <th>Shipper</th>
                 <th>Consigner</th>
                 <th>Remark</th>
               

            </tr>
      
       
      
            @foreach (var item in Model)
            {
                <tr>
                    
                    <th>@item.JobCode</th>
                    <th>@item.JobDate</th>
                    <th>@item.shipper</th>
                    <th>@item.Customer</th>
                    <th>@item.Remarks</th>
                   
                    
                </tr>
            }
     
    </table>


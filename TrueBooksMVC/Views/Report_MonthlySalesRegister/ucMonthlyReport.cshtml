@*@model IEnumerable<DAL.qrySupplierOutstanding>*@
@using TrueBooksMVC.Models;
@using DAL;
@using System.Text;
@using System.Configuration;

@{
    Layout = null;

   
  
}

<div class="row">
    <div class="col-md-12">
        @{
            int custid = Convert.ToInt32(ViewBag.custid);
            int empid = Convert.ToInt32(ViewBag.empid);
            DateTime fromdate = Convert.ToDateTime(ViewBag.fromdate);
            DateTime todate = Convert.ToDateTime(ViewBag.todate);

            string path = ConfigurationManager.AppSettings["spath"].ToString();
            string url = path + "/Reports/MonthlySalesReport.aspx?custid=" + custid + "&empid=" + empid + "&fromdate=" + fromdate + "&todate=" + todate;
            
            <iframe src="@url" style="width:100%;height:1500px"></iframe>
        }
    </div>
</div>




